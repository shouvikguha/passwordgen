{
    "manifest_version": 3,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Generate secure passwords easily",
    "permissions": ["clipboardWrite", "activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "assets/favicon_io/favicon-16x16.png",
        "32": "assets/favicon_io/favicon-32x32.png"
      }
    },
    "icons": {
      "16": "assets/favicon_io/favicon-16x16.png",
      "32": "assets/favicon_io/favicon-32x32.png"
    }
  }
  